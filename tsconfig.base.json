{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@stt/features/dashboard/feature": [
        "libs/features/dashboard/feature/src/index.ts"
      ],
      "@stt/features/person/domain": [
        "libs/features/person/domain/src/index.ts"
      ],
      "@stt/features/person/feature": [
        "libs/features/person/feature/src/index.ts"
      ],
      "@stt/features/person/ui": ["libs/features/person/ui/src/index.ts"],
      "@stt/features/person/util": ["libs/features/person/util/src/index.ts"],
      "@stt/features/settings/feature": [
        "libs/features/settings/feature/src/index.ts"
      ],
      "@stt/features/shell/feature": [
        "libs/features/shell/feature/src/index.ts"
      ],
      "@stt/features/shell/model": ["libs/features/shell/model/src/index.ts"],
      "@stt/features/shell/ui": ["libs/features/shell/ui/src/index.ts"],
      "@stt/features/training/domain": [
        "libs/features/training/domain/src/index.ts"
      ],
      "@stt/features/training/feature": [
        "libs/features/training/feature/src/index.ts"
      ],
      "@stt/features/training/model": [
        "libs/features/training/model/src/index.ts"
      ],
      "@stt/shared/button/ui": ["libs/shared/button/ui/src/index.ts"],
      "@stt/shared/config/model": ["libs/shared/config/model/src/index.ts"],
      "@stt/shared/http/domain": ["libs/shared/http/domain/src/index.ts"],
      "@stt/shared/icon/domain": ["libs/shared/icon/domain/src/index.ts"],
      "@stt/shared/icon/model": ["libs/shared/icon/model/src/index.ts"],
      "@stt/shared/person/domain": ["libs/shared/person/domain/src/index.ts"],
      "@stt/shared/person/model": ["libs/shared/person/model/src/index.ts"],
      "@stt/shared/routing/model": ["libs/shared/routing/model/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
